                    <p class="text-sm mb-0 text-uppercase font-weight-bold">Prepagas</p>
                    <h5 class="font-weight-bolder" id="turnosprepagas">
                      {{#if turnosprepagas}}
                        {{turnosprepagas.0.total}}
                      {{else}}
                        0
                      {{/if}}
                    </h5>
                   
                  </div>
                </div>
                <div class="col-4 text-end">
                  <div class="icon icon-shape bg-gradient-success shadow-success text-center rounded-circle">
                    <i class="ni ni-paper-diploma text-lg opacity-10" aria-hidden="true"></i>
                  </div>
                </div>
              </div>
            </div>
          </div>
