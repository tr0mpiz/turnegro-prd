        <div class="col-xl-3 col-sm-6">
          <div class="card">
            <div class="card-body p-3">
              <div class="row">
                <div class="col-8">
                  <div class="numbers">
                    <p class="text-sm mb-0 text-uppercase font-weight-bold">Total</p>
                    <h5 class="font-weight-bolder" id="turnostotal">
                     {{#if turnostotal.0.total}}
                        {{turnostotal.0.total}}
                      {{else}}
                        0
                      {{/if}}
                    </h5>
                  </div>
                </div>
                <div class="col-4 text-end">
                  <div class="icon icon-shape bg-gradient-warning shadow-warning text-center rounded-circle">
                    <i class="ni ni-cart text-lg opacity-10" aria-hidden="true"></i>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="row mt-4">
        <div class="col-lg-9 mb-lg-0 mb-4">
          <div class="card z-index-2 h-100">
            <div class="card-header pb-0 pt-3 bg-transparent">
              <h4 class="text-capitalize ">tickets creados</h4>
            </div>
            <div class="card-body p-3">
              <div class="table-responsive">
              <table class="table align-items-center" id="dataTable">
                <thead>
                    <tr>
                      <th>Numero</th>
                      <th>Llegada</th>
                      <th>Hora</th>
                      <th class="text-center">Requisitos</th>
                    </tr>
                </thead>
                
                <tbody id="table-proximo">
                  {{#each turnos}}
                      <tr data-bs-id="{{ this.id }}" data-bs-numero="{{ this.primera_letra }}{{ this.ultima_letra }}-{{ this.id }}" data-bs-toggle="modal" data-bs-target="#turnoModal">  
                          <td>
                              <h6 class="mb-0 text-sm">{{ this.primera_letra }}{{ this.ultima_letra }}-{{ this.id }}</h6>
                              <p class="text-xs text-secondary mb-0">{{ this.nombretramite }}</p>
                          </td>
                          <td>
                            <p class="mb-0 text-sm">{{ this.fechaf }}</p>
                            <p class="text-xs text-secondary mb-0">{{ this.hora }}</p>
                          </td>
                          <td class="align-middle text-center text-sm">
                            <span class="badge badge-sm bg-gradient-success">{{ this.nombreestado }}</span>
                          </td>
                          <td class="align-middle text-center">
                            <span class="text-secondary text-xs font-weight-bold">{{ this.requisitos }}</span>
                          </td>
                        </tr>
                        {{/each}}
                </tbody>
              </table>
            </div>
            </div>
          </div>
        </div>
        <div class="col-lg-3">
          <div class="card z-index-2 h-100">
            <div class="card-header pb-0 p-3">
              <h6 class="mb-0 text-center">Personas atendidas </h6>
          
            </div>
            <div class="card-body p-3">
              <ul class="list-group">
                {{#each tramitesTotalesAgrupadosYSumados}}
                  <li class="list-group-item border-0 d-flex justify-content-between ps-0 border-radius-lg">
                    <div class="d-flex align-items-center">
                      <div class="icon icon-shape icon-sm me-3 bg-gradient-primary shadow text-center">
                        <i class="ni ni-ambulance text-white opacity-20"></i>
                      </div>
                      <div class="d-flex flex-column">
                        <h6 class="mb-1 text-dark text-sm">{{ this.nombre }}</h6>
                        <span class="text-xs font-weight-bold">{{ this.cantidad }}</span>
                      </div>
                    </div>
                    <div class="d-flex">
                      <button class="btn btn-link btn-icon-only btn-rounded btn-sm text-dark icon-move-right my-auto"><i class="ni ni-bold-right" aria-hidden="true"></i></button>
                    </div>
                  </li>
                {{/each}}
                <li class="list-group-item border-0 d-flex justify-content-between ps-0 border-radius-lg">
                  <div class="d-flex align-items-center">
                    
                  </div>
                  <div class="d-flex">
                    <button class="btn btn-xs btn-icon  btn-primary" type="button"><span class="btn-inner--icon"><i class="ni ni-fat-add"></i >Agregar Grupo</span> </button>
                  </div>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      
