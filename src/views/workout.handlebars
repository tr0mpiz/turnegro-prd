<div class="modal fade" id="youTubeModal" tabindex="-1" style="display: none;" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <iframe height="350" src=""></iframe>
    </div>
  </div>
</div>

<h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">ENTRENAMIENTO /</span> RUTINA </h4>
<div class="container agendacontenedor">
 
  <div class="row">
    <!-- Earnings (Monthly) Card Example -->
  <div class="col-md-12 mb-4">
        <div class="card">
            <div class="d-flex align-items-end row">
                <div class="col-sm-7">
                <div class="card-body">
                    <h5 class="card-title text-primary">RUTINA </h5>
                    <p class="mb-4">En esta <span class="fw-bold">seccion</span> tus ejercicios</p>
                </div>
                </div>
                <div class="col-sm-5 text-center text-sm-left d-none d-sm-block">
                <div class="card-body pb-0 px-0 px-md-4 d-none d-sm-block">
                    <img src="../../../storage/icono/pesa.png" height="140" alt="View Badge User" data-app-dark-img="illustrations/man-with-laptop-dark.png" data-app-light-img="illustrations/man-with-laptop-light.png">
                </div>
                </div>
            </div>
        </div>
    </div>
  
    <!-- DataTales Example -->


   
    {{#each ejercios}}
      <div class="col-12 col-sm-6 col-lg-4 mb-4">
        <div class="card rutina-{{this.id_rutina}} {{#compare this.n_completado 1}} disabled-card  {{/compare}}">
          <div class="card-body text-center">
            <i class="mb-4 mt-2 bx bx-md bx-dumbbell"></i>
            <h2 id="ejercicio-{{this.id_rutina}}" class="card-title">{{this.nombre}}</h2>
            <p class="m-4 tecnica" > {{{this.tecnica}}}</p>
            <div class="mt-4">
               <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#youTubeModal" data-thevideo="{{this.url}}"> Ver tecnica</button>
            </div>
            <div class="mt-3 mb-3">
              <button type="button" class="btn btn-info" onclick="startTimer({{this.id_rutina}})"> Iniciar Temporizador ‚è±Ô∏è</button>
              <div id="timer-display-{{this.id_rutina}}" class="m-2"></div>
            </div>
            <label class="form-check-label" for="brandingCheckbox">
              
              <div>
                <span class="btn btn-xs btn-primary " id="descanso-{{this.id_rutina}}">DESCANSO DE  {{this.n_descanso}} SEGUNDOS </span>
              </div>
              <div>
                <span class="btn btn-xs btn-primary " id="repeticiones-{{this.id_rutina}}">   {{this.n_repeticion}} REPETICIONES </span>
              </div>
              <div>
              
                <span class="btn btn-xs btn-primary " id="series-{{this.id_rutina}}"> {{this.n_serie}} SERIES </span>
              </div>
              
              <div class="mb-5">
                <span class="btn btn-xs btn-primary " id="peso-{{this.id_rutina}}"> {{this.n_peso}} KILOS </span>
              </div>
            </label>
            

            
            <div class="">
               <button type="button" class="btn  botonActualizaEstado {{#compare this.n_completado 0}} btn-success {{/compare}} {{#compare this.n_completado 1}} btn-secondary {{/compare}}" onclick="actualizaEstado({{this.id_rutina}})">{{#compare this.n_completado 0}} Termine üéâ {{/compare}} {{#compare this.n_completado 1}} Retomar üí™ {{/compare}}</button>
            </div>
            
            
           
              
          </div>
        </div>
      </div>
    {{/each}}
    
    
    
</div>



